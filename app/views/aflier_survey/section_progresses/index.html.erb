<p id="notice"><%= notice %></p>

<h1>Section Progresses</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Question section</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_progresses.each do |section_progress| %>
      <tr>
        <td><%= section_progress.user_id %></td>
        <td><%= section_progress.question_section_id %></td>
        <td><%= section_progress.status %></td>
        <td><%= link_to 'Show', section_progress %></td>
        <td><%= link_to 'Edit', edit_section_progress_path(section_progress) %></td>
        <td><%= link_to 'Destroy', section_progress, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section Progress', new_section_progress_path %>
