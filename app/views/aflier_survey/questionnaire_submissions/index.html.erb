<p id="notice"><%= notice %></p>

<h1>Questionnaire Submissions</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Questionnaire</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionnaire_submissions.each do |questionnaire_submission| %>
      <tr>
        <td><%= questionnaire_submission.unique_ident %></td>
        <td><%= questionnaire_submission.questionnaire_id %></td>
        <td><%= questionnaire_submission.status %></td>
        <td><%= link_to 'Show', questionnaire_submission %></td>
        <td><%= link_to 'Edit', edit_questionnaire_submission_path(questionnaire_submission) %></td>
        <td><%= link_to 'Destroy', questionnaire_submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questionnaire Submission', new_questionnaire_submission_path %>
