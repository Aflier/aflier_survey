<p id="notice"><%= notice %></p>

<p>Extracted from Questionnaire: <%= AflierSurvey::Questionnaire.find_answer_by_key("user-#{@user.id}", 'KEY-ONE') %></p>
<p>Keys from Questionnaire: <%= AflierSurvey::Questionnaire.all_keys.join(', ') %></p>


<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>

<div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-5">
    <%= render 'aflier_survey/questionnaires/questionnaire',
               questionnaire: AflierSurvey::Questionnaire.all.first,
               unique_ident: "user-#{@user.id}", admin: false %>
  </div>
  <div class="col-md-5">
    Admin
    <%= render 'aflier_survey/questionnaires/questionnaire',
               only_answers: true,
               questionnaire: AflierSurvey::Questionnaire.all.first,
               unique_ident: "user-#{@user.id}", admin: true %>


    Not Admin
    <%= render 'aflier_survey/questionnaires/questionnaire',
               only_answers: true,
               questionnaire: AflierSurvey::Questionnaire.all.first,
               unique_ident: "user-#{@user.id}", admin: false %>

  </div>
</div>
